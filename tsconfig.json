{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "outDir": "dist",
      "rootDir": "./practice/test",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "declaration": true,
      "forceConsistentCasingInFileNames": true,
    },
    "include": ["practice/test/**/*.ts"],
    "exclude": ["node_modules", "dist"]
  }
  